{
  "name": "uni-app-snippets-vscode",
  "displayName": "uni-app-snippets",
  "version": "0.9.5",
  "description": "uni-app 基本能力代码片段",
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "uniapp",
    "uni-app",
    "snippet",
    "snippets"
  ],
  "homepage": "https://github.com/uni-helper/uni-app-snippets-vscode#readme",
  "bugs": {
    "url": "https://github.com/uni-helper/uni-app-snippets-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uni-helper/uni-app-snippets-vscode"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "publisher": "uni-helper",
  "type": "module",
  "scripts": {
    "check:deps": "ncu",
    "check:types": "tsc --noEmit",
    "lint": "run-p lint:eslint lint:markdownlint check:types",
    "lint:eslint": "eslint . --ext=.js,.cjs,.mjs,.ts,.cts,.mts --fix --cache --no-error-on-unmatched-pattern --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "release": "release-it",
    "prepare": "is-ci || husky install",
    "publish": "run-p publish:vscode publish:openvsx",
    "publish:vscode": "vsce package && vsce publish",
    "publish:openvsx": "ovsx publish"
  },
  "contributes": {
    "snippets": [
      {
        "language": "vue-html",
        "path": "./snippets/vue-html.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue-html.json"
      },
      {
        "language": "html",
        "path": "./snippets/vue-html.json"
      },
      {
        "language": "css",
        "path": "./snippets/css.json"
      },
      {
        "language": "less",
        "path": "./snippets/css.json"
      },
      {
        "language": "scss",
        "path": "./snippets/css.json"
      },
      {
        "language": "sass",
        "path": "./snippets/css.json"
      },
      {
        "language": "stylus",
        "path": "./snippets/css.json"
      },
      {
        "language": "vue",
        "path": "./snippets/css.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "vue",
        "path": "./snippets/javascript.json"
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@commitlint/prompt": "^17.4.0",
    "@modyqyw/fabric": "^5.15.0",
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "commitizen": "^4.2.6",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "markdownlint-cli": "^0.32.2",
    "npm-check-updates": "^16.6.2",
    "npm-run-all": "^4.1.5",
    "ovsx": "^0.8.0",
    "prettier": "^2.8.1",
    "release-it": "^15.6.0",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4",
    "vsce": "^2.14.0"
  },
  "packageManager": "npm@9.2.0",
  "engines": {
    "node": ">=14.18",
    "vscode": "^1.40.0"
  },
  "icon": "uni-helper.png",
  "sponsor": {
    "url": "https://www.paypal.me/wurui7"
  }
}
